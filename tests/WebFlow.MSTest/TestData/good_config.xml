<?xml version="1.0" encoding="utf-8"?>
<project xmlns="https://raw.githubusercontent.com/Ackara/WebFlow/master/src/WebFlow/intellisense.xsd"
         name="test"
         outputDirectory="%temp%\webflow">
  <typescript>
    <include outFile="shared/_layout.*html">
      <pattern>wwwroot/**/*.ts</pattern>
    </include>

    <exclude>
      <pattern>_*.ts</pattern>
    </exclude>
  </typescript>

  <sass />

  <image>
    <optimize compression="lossless">
      <pattern>*.jpg</pattern>
    </optimize>

    <optimize compression="lossy" quality="50">
      <pattern>*.png</pattern>
    </optimize>
  </image>
</project>