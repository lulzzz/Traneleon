  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <LangVersion>7.2</LangVersion>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Acklann.NShellit" Version="0.0.15-rc" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\WebFlow\WebFlow.csproj" />
